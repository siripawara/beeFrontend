{
  "expo": {
    "name": "Bee Alert",
    "slug": "beebox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.siripawara.beebox"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c6a2f8e4-5a7e-49be-a5d1-ab3453c0bd75"
      }
    },
    "plugins": [
      [
        "expo-updates",
        {
          "username": "siripawara"
        }
      ]
    ]
  }
}
